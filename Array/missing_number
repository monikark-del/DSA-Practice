#include<iostream>
using namespace std;
int missing_number(int a[],int n)
{
   int XOR=0;
   for(int i=0;i<=n;i++)
   {
       XOR^=a[i];
   }
   for(int i=0;i<=n;i++)
   {
       XOR^=i;
   }
   return XOR;
}
int main()
{
    int a[]={2,1,3,4,5};
    int n=sizeof(a)/sizeof(a[0]);
    int result=0;
    result=missing_number(a,n);
    cout<<"missing number is:"<<result;
    return 0;
}
