#include<iostream>
using namespace std;
int strstr(string haystack,string needle)
{
	if (needle=="")
	    return 0;
	int occurance=haystack.find(needle);
	if(occurance!=string::npos)
	    return occurance;
	return -1;
}
int main()
{
	string haystack="helloMyNameIsMonika";
	string needle="Monika";
	int found=strstr(haystack,needle);
	cout<<"found="<<found;
}
