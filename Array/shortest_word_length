#include<iostream>
#include<vector>
#include<cmath>
#include<algorithm>
using namespace std;
int s_w_d(vector<string> v,string word1,string word2)
{
   int index1=-1;
   int index2=-1;
   int ans=INT_MAX;
   for(int i=0;i<v.size();i++)
   {
       if(v[i]==word1)
         index1=i;
       if(v[i]==word2)
          index2=i;
       if(index1!=-1&&index2!=-1)
          ans=min(ans,abs(index1-index2));
   }
   return ans;
}
int main()
{
    string a[]={"practice","makes","perfect","coding","makes"};
    vector<string> s(a,a+sizeof(a)/sizeof(a[0]));
    string word1="makes";
    string word2="coding";
    int result=s_w_d(s,word1,word2);
    cout<<result;
    return 0;
}
