#include<iostream>
using namespace std;
void Plusone(int a[],int n)
{
    for(int i=n-1;i>=0;i--)
    {
        if(a[i]+1<10)
        {
            a[i]++;
            for(int i=0;i<n;i++)
            {
              cout<<a[i];
            }

        return;
        }
        else
        {
            a[i]=0;
        }
    }
    cout<<"1";
    for(int i=0;i<n;i++)
    {
        cout<<a[i];
    }
}
int main()
{

    int n;
    cout<<"enter n value:";
    cin>>n;
    int a[100];
    cout<<"enter array elements:"<<endl;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    Plusone(a,n);
    return 0;
}

