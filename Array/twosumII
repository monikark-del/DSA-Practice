#include<iostream>
#include<utility>
using namespace std;
pair<int,int> twoSum(int a[],int n)
{
    int target=10;
    for(int i=0;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(a[i]+a[j]==target)
                return make_pair(i,j);
        }
    }

}
int main()
{
   int a[]={1,3,4,5,6,7};
   int n=sizeof(a)/sizeof(a[0]);
   pair<int,int> result=twoSum(a,n);
   cout<<result.first<<","<<result.second;
   return 0;

}
